#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here --This code interacts with the CLI
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
location = gets.strip
move(board, location, 'X')
display_board(board)