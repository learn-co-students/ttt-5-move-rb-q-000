#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
puts "Please input a square ID, with 1 in the top left and 9 in the bottom right."
position = gets.strip

move(board, position)

display_board(board)
