#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"
input = gets.strip

# Call #move with arguments from #board,
# the user's input and default player of "X"
move(board, input, "X")


# Call #display_board with necessary arguments
display_board(board)
