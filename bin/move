#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
puts "What position would you like to fill? Choose 1-9."
position = gets.strip
index = input_to_index(position)

my_move = move(board, index, "X")

display_board(board)
